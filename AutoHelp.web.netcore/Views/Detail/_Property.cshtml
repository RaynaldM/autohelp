<div class="reduceDivScroll list-group">
    {{#each Properties}}
    <a href="#" data-type="0" data-subtype="2" data-id="{{Id}}" data-parent="{{ParentClass}}" data-namespace="{{../namespace}}" class="list-group-item popoverselect">
        <h6 class="list-group-item-heading hideOverflow">{{Name}}</h6>
        <div class="list-group-item-text">
            <small>{{Summary}}</small>
        </div>
    </a>
    {{/each}}
</div>
