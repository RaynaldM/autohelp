                {{/ifGreater}}
                {{#ifGreater StructureCount 0}}
                <a class="btn btn-xs btn-info" href="#" data-id="{{Name}}" data-type="1" title="# of structures">{{StructureCount}} Structures</a>
                {{/ifGreater}}
                {{#ifGreater InterfaceCount 0}}
                <a class="btn btn-xs btn-primary" href="#" data-id="{{Name}}" data-type="2" title="# of interfaces">{{InterfaceCount}} Interfaces</a>
                {{/ifGreater}}
                {{#ifGreater EnumerationCount 0}}
                <a class="btn btn-xs btn-primary" href="#" data-id="{{Name}}" data-type="3" title="# of enumerations">{{EnumerationCount}} Enums</a>
                {{/ifGreater}}
                {{#ifGreater DelegateCount 0}}
                <a class="btn btn-xs btn-primary" href="#" data-id="{{Name}}" data-type="4" title=" # of delegates">{{DelegateCount}} Delegates</a>
                {{/ifGreater}}
            </div>
        </li>*@
    {{/each}}
</script>
