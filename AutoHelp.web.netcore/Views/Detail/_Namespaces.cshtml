<script id="namespaces-template" type="text/x-handlebars-template">
    {{#each this}}
    <div href="#" class="list-group-item list-group-item-action flex-column align-items-start">
        <div class="d-flex w-100 justify-content-between">
            <h6 class="mb-1 text-truncate" title="{{Name}}">{{Name}}</h6>
        </div>
        <p class="mb-1">
            {{#ifGreater ClassCount 0}}
            <a class="btn btn-sm btn-warning" href="#" data-id="{{Name}}" data-type="0" title="# of classes">{{ClassCount}} Classes</a>
            {{/ifGreater}}
            {{#ifGreater StructureCount 0}}
            <a class="btn btn-sm btn-info" href="#" data-id="{{Name}}" data-type="1" title="# of structures">{{StructureCount}} Structures</a>
            {{/ifGreater}}
            {{#ifGreater InterfaceCount 0}}
            <a class="btn btn-sm btn-primary" href="#" data-id="{{Name}}" data-type="2" title="# of interfaces">{{InterfaceCount}} Interfaces</a>
            {{/ifGreater}}
            {{#ifGreater EnumerationCount 0}}
            <a class="btn btn-sm btn-default" href="#" data-id="{{Name}}" data-type="3" title="# of enumerations">{{EnumerationCount}} Enums</a>
            {{/ifGreater}}
            {{#ifGreater DelegateCount 0}}
            <a class="btn btn-sm btn-primary" href="#" data-id="{{Name}}" data-type="4" title=" # of delegates">{{DelegateCount}} Delegates</a>
            {{/ifGreater}}
        </p>
    </div>
    @*<li class="list-group-item">
            <h5 class="list-group-item-heading hideOverflow">{{Name}}</h5>
            <div class="list-group-item-text">
                {{#ifGreater ClassCount 0}}
